{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/assets/css/**",
          "/assets/images/**",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/assets/images/**",
          "/assets/images/icon-app/**",
          "/**.(eot|svg|cur|jpg|jpeg|png|PNG|webp|gif|otf|ttf|woff|woff2|ani)"
        ],
        "urls": [
          "**/file/**.jpg",
          "**/file/**.jpeg",
          "**/file/**.png",            
          "**/print/logo/**.jpg",
          "**/print/logo/**.png"
        ]
      }
    }
  ],
  "dataGroups": [
    {
        "name": "dynamicResources",
        "urls": [
            "**/file/**.jpg",
            "**/file/**.jpeg",
            "**/file/**.png",            
            "**/print/logo/**.jpg",
            "**/print/logo/**.png"
        ],
        "cacheConfig": {
            "strategy": "performance",
            "maxSize": 5500,
            "maxAge": "7d"
        }        
    }
]
}
